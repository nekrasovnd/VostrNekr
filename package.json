{
  "name": "simple-calculator",
  "version": "1.0.0",
  "description": "A simple calculator with code analysis and coverage",
  "main": "js/calculator.js",
  "scripts": {
    "test": "node test/coverage-test.js",
    "coverage": "npx c8 --include=\"js/calculator.js\" --reporter=text node test/coverage-test.js",
    "coverage:html": "npx c8 report --include=\"js/calculator.js\" --reporter=html"
  },
  "keywords": ["calculator", "coverage", "testing"],
  "author": "VostrNekr",
  "license": "MIT",
  "devDependencies": {
    "c8": "^8.0.0"
  }
}
